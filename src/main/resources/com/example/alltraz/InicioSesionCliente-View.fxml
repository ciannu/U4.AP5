<AnchorPane fx:id="rootPane" prefHeight="598.0" prefWidth="875.0" style="-fx-background-color: #a1cffb;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.alltraz.InicioSesionClienteController">
   <children>
      <ImageView fitHeight="598.0" fitWidth="875.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../Images/Diseño%20sin%20título.jpg" />
         </image>
      </ImageView>
      <ImageView fitHeight="71.0" fitWidth="94.0" layoutX="484.0" layoutY="508.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../Images/BotonRegistrar.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="71.0" fitWidth="62.0" layoutX="771.0" layoutY="513.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../Images/flecha.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="139.0" fitWidth="337.0" layoutX="267.0" layoutY="27.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../Images/IconoHorizontal-removebg-preview.png" />
         </image>
      </ImageView>
      <TextField fx:id="txtUsuername" layoutX="275.0" layoutY="249.0" onKeyTyped="#eventkey" prefHeight="26.0" prefWidth="324.0">
         <effect>
            <DropShadow />
         </effect>
         <cursor>
            <Cursor fx:constant="TEXT" />
         </cursor></TextField>
      <Text fill="WHITE" layoutX="397.0" layoutY="236.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Username:">
         <font>
            <Font name="Agency FB" size="25.0" />
         </font>
      </Text>
      <TextField fx:id="txtPassword" layoutX="275.0" layoutY="334.0" onKeyTyped="#eventkey" prefHeight="26.0" prefWidth="324.0">
         <effect>
            <DropShadow />
         </effect>
         <cursor>
            <Cursor fx:constant="TEXT" />
         </cursor></TextField>
      <ImageView fitHeight="71.0" fitWidth="82.0" layoutX="406.0" layoutY="376.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../Images/flecha-correcta.png" />
         </image>
      </ImageView>
      <Button fx:id="registrarse" layoutX="406.0" layoutY="379.0" mnemonicParsing="false" onAction="#click_InicioSesion" prefHeight="64.0" prefWidth="71.0" style="-fx-background-color: transparent;">
         <font>
            <Font name="Monospaced Regular" size="17.0" />
         </font>
         <effect>
            <Lighting diffuseConstant="1.4">
               <bumpInput>
                  <Shadow />
               </bumpInput>
               <light>
                  <Light.Distant />
               </light>
            </Lighting>
         </effect>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button fx:id="atras" layoutX="771.0" layoutY="513.0" mnemonicParsing="false" onAction="#click_atras" prefHeight="62.0" prefWidth="62.0" style="-fx-background-color: transparent;">
         <font>
            <Font size="19.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button fx:id="Registrarse" layoutX="478.0" layoutY="511.0" mnemonicParsing="false" onAction="#click_Registrarse" prefHeight="64.0" prefWidth="83.0" style="-fx-background-color: transparent;">
         <font>
            <Font name="Monospaced Regular" size="12.0" />
         </font>
         <effect>
            <Reflection />
         </effect>
         <cursor>
            <Cursor fx:constant="HAND" />
